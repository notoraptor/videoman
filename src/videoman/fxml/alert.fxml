<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" minHeight="-Infinity" minWidth="-Infinity"
          xmlns="http://javafx.com/javafx/8.0.65" fx:controller="videoman.control.AlertController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="1.7976931348623157E308" vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
    </rowConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label fx:id="title" maxWidth="1.7976931348623157E308" text="Label" GridPane.columnSpan="2">
        <font>
            <Font name="System Bold" size="20.0"/>
        </font>
    </Label>
    <Label fx:id="message" maxWidth="1.7976931348623157E308" text="Label" wrapText="true" GridPane.columnSpan="2"
           GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
    <Button fx:id="button" mnemonicParsing="false" text="OK" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
            GridPane.rowIndex="2"/>
</GridPane>
